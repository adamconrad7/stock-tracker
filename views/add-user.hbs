<h2>{{message}}</h2>

<form method="post" id="addForm">
    <fieldset>
        <legend>User Info:</legend>
        <p>
            <label for="fName">First Name:</label>
            <input type="text"  class="required" name="fName" id="fName" >
        </p>
        <p>
            <label for="lName">Last name:</label>
            <input type="text"  class="required" name="lName" id="lName" >
        </p>
        <p>
            <label for="sID">Username:</label>
            <input type="text"  class="required" name="uName" id="uName" title="sid should be numeric">
        </p>
        <p>
            <label for="Name">Password:</label>
            <input type="password" class="required" name="uPass" id="uPass">
        </p>

      
    </fieldset>

    <p>
        <input type="submit" value="Submit" />
        <input type="reset" value="Clear Form" />
    </p>
</form>
<script type="text/javascript" src="/assets/verify-input.js"></script>
